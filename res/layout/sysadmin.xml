<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
    <LinearLayout
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:background="@color/button_unselected"
            android:layout_width="fill_parent"
            android:layout_height="44.0dip">

        <TextView
                android:textSize="18.0sp"
                android:textColor="@android:color/white"
                android:padding="12.0dip"
                android:layout_width="fill_parent"
                android:layout_height="46.0dip"
                android:gravity="center"
                android:text="系统管理"/>

    </LinearLayout>


    <ScrollView
            android:scrollbarThumbVertical="@drawable/scrollbar_handle_vertical"
            android:background="@color/info_panel_bg"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1.0">

        <LinearLayout
                android:orientation="vertical"
                android:paddingBottom="10.0dip"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:isScrollContainer="true">





            <!-- 第二个白框begin -->
            <LinearLayout
                    android:gravity="center_vertical"
                    android:orientation="vertical"
                    android:background="@drawable/dark_circular"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10.0dip"
                    android:layout_marginTop="7.0dip"
                    android:layout_marginRight="10.0dip">
                <LinearLayout
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:id="@+id/sysadmin_extract_linearLayout"
                        android:layout_width="fill_parent"
                        android:layout_height="50.0dip">
                    <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10.0dip"
                            android:src="@drawable/workset"/>
                    <TextView
                            android:textSize="18.0sp"
                            android:textColor="#ff545454"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10.0dip"
                            android:text="提取处方"
                            android:layout_weight="1.0"/>
                    <ImageView
                            android:layout_width="12.0dip"
                            android:layout_height="12.0dip"
                            android:layout_marginRight="10.0dip"
                            android:src="@drawable/into_icon"/>
                </LinearLayout>
            </LinearLayout>
            <!-- 第二个白框end -->

            <!-- 第三个白框begin -->
            <LinearLayout
                    android:gravity="center_vertical"
                    android:orientation="vertical"
                    android:background="@drawable/dark_circular"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10.0dip"
                    android:layout_marginTop="7.0dip"
                    android:layout_marginRight="10.0dip">
                <LinearLayout
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:id="@+id/sysadmin_topPre_linearLayout"
                        android:layout_width="fill_parent"
                        android:layout_height="50.0dip">
                    <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10.0dip"
                            android:src="@drawable/workset"/>
                    <TextView
                            android:textSize="18.0sp"
                            android:textColor="#ff545454"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10.0dip"
                            android:text="优先删除处方"
                            android:layout_weight="1.0"/>
                    <ImageView
                            android:layout_width="12.0dip"
                            android:layout_height="12.0dip"
                            android:layout_marginRight="10.0dip"
                            android:src="@drawable/into_icon"/>
                </LinearLayout>
                <View
                        android:background="@drawable/shape_line"
                        android:layout_width="fill_parent"
                        android:layout_height="2px"/>
                <LinearLayout
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:id="@+id/sysadmin_assignWork_linearLayout"
                        android:layout_width="fill_parent"
                        android:layout_height="50.0dip">
                    <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10.0dip"
                            android:src="@drawable/workset"/>
                    <TextView
                            android:textSize="18.0sp"
                            android:textColor="#ff545454"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10.0dip"
                            android:text="分配预发处方"
                            android:layout_weight="1.0"/>
                    <ImageView
                            android:layout_width="12.0dip"
                            android:layout_height="12.0dip"
                            android:layout_marginRight="10.0dip"
                            android:src="@drawable/into_icon"/>
                </LinearLayout>
            </LinearLayout>
            <!-- 第三个白框end -->
            <!-- 第四个白框begin -->
            <LinearLayout
                    android:gravity="center_vertical"
                    android:orientation="vertical"
                    android:background="@drawable/dark_circular"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10.0dip"
                    android:layout_marginTop="7.0dip"
                    android:layout_marginRight="10.0dip">
                <LinearLayout
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:id="@+id/sysadmin_online_linearLayout"
                        android:layout_width="fill_parent"
                        android:layout_height="50.0dip">
                    <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10.0dip"
                            android:src="@drawable/workset"/>
                    <TextView
                            android:textSize="18.0sp"
                            android:textColor="#ff545454"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10.0dip"
                            android:text="查看在线用户"
                            android:layout_weight="1.0"/>
                    <ImageView
                            android:layout_width="12.0dip"
                            android:layout_height="12.0dip"
                            android:layout_marginRight="10.0dip"
                            android:src="@drawable/into_icon"/>
                </LinearLayout>
            </LinearLayout>
            <!-- 第四个白框end -->
            <!-- 第伍个白框begin -->
            <LinearLayout
                    android:gravity="center_vertical"
                    android:orientation="vertical"
                    android:background="@drawable/dark_circular"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10.0dip"
                    android:layout_marginTop="7.0dip"
                    android:layout_marginRight="10.0dip">
                <LinearLayout
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:id="@+id/sysadmin_waitDispen_linearLayout"
                        android:layout_width="fill_parent"
                        android:layout_height="50.0dip">
                    <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10.0dip"
                            android:src="@drawable/workset"/>
                    <TextView
                            android:textSize="18.0sp"
                            android:textColor="#ff545454"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10.0dip"
                            android:text="查看待调处方"
                            android:layout_weight="1.0"/>
                    <ImageView
                            android:layout_width="12.0dip"
                            android:layout_height="12.0dip"
                            android:layout_marginRight="10.0dip"
                            android:src="@drawable/into_icon"/>
                </LinearLayout>
                <View
                        android:background="@drawable/shape_line"
                        android:layout_width="fill_parent"
                        android:layout_height="2px"/>
                <LinearLayout
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:id="@+id/sysadmin_selectPres_linearLayout"
                        android:layout_width="fill_parent"
                        android:layout_height="50.0dip">
                    <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10.0dip"
                            android:src="@drawable/workset"/>
                    <TextView
                            android:textSize="18.0sp"
                            android:textColor="#ff545454"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10.0dip"
                            android:text="查看处方状态"
                            android:layout_weight="1.0"/>
                    <ImageView
                            android:layout_width="12.0dip"
                            android:layout_height="12.0dip"
                            android:layout_marginRight="10.0dip"
                            android:src="@drawable/into_icon"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>
